[alias]
	st = status
	co = checkout
	br = branch
	p = pull --rebase
	cm = commit -m
	ap = add -p
	ad = add -u
	l = log
	sh = stash
	sa = stash apply
	sl = shortlog -s -n --all
        d = diff
[push]
	default = simple
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
